    "e15-4yp-Optimizing-chloroplast-genome-assembly-and-annotation-with-skim-sequencing-data": {
        "projName": "Optimizing chloroplast genome assembly and annotation with skim sequencing data",
        "batch": "E15",
        "category": {
            "title": "Undergraduate Research Projects",
            "code": "4yp",
            "api_url": "https://api.ce.pdn.ac.lk/projects/v1/4yp/"
